{
  "name": "loya-crm-frontend",
  "version": "0.1.15",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development && next dev",
    "build": "next build",
    "start": "next start",
    "dev:debug": "NODE_ENV=development && NODE_OPTIONS='--inspect' next dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "lint:check": "eslint --max-warnings 25 .",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "type-check": "tsc --noEmit && tsc --noEmit --project tsconfig.test.json",
    "type-check:watch": "tsc --noEmit --watch",
    "pretype-check": "pnpm run collect-locales",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:lead-dialog": "jest --testPathPatterns=LeadEditDialog",
    "test:ui": "jest --verbose",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:demo": "cypress run --spec cypress/e2e/demo-cypress.cy.ts",
    "test:e2e": "cypress run --spec cypress/e2e/lead-edit-dialog.cy.ts",
    "test:e2e:deal": "cypress run --spec cypress/e2e/deal-edit-dialog.cy.ts",
    "test:e2e:lead": "cypress run --spec cypress/e2e/lead-edit-dialog.cy.ts",
    "test:e2e:open": "cypress open --e2e",
    "playwright": "playwright test",
    "playwright:ui": "playwright test --ui",
    "playwright:headed": "playwright test --headed",
    "playwright:demo": "playwright test e2e/demo-playwright.spec.ts",
    "playwright:deal": "playwright test e2e/deal-edit-dialog.spec.ts",
    "playwright:lead": "playwright test e2e/lead-edit-dialog.spec.ts",
    "playwright:management": "playwright test e2e/lead-management.spec.ts",
    "playwright:report": "playwright show-report",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:cache": "rm -rf .next/cache",
    "collect-locales": "node scripts/collect-locales.js",
    "predev": "pnpm run clean && pnpm run collect-locales && node scripts/ensure-no-legacy-pages.js",
    "prebuild": "pnpm run collect-locales && node scripts/ensure-no-legacy-pages.js",
    "prebuild:dev": "pnpm run clean && pnpm run collect-locales && node scripts/ensure-no-legacy-pages.js",
    "analyze": "ANALYZE=true next build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy": "cd .. && ./deploy.sh"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@hookform/resolvers": "^5.2.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-data-grid": "^8.10.0",
    "@mui/x-date-pickers": "^8.10.0",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "@types/react-dom": "^18.3.7",
    "dayjs": "^1.11.13",
    "i18next": "^25.4.2",
    "next": "^15.4.6",
    "notistack": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.0",
    "@cypress/webpack-dev-server": "^5.1.2",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.5.0",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-a11y": "^9.1.1",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/addon-vitest": "^9.1.1",
    "@storybook/nextjs-vite": "^9.1.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "^18.3.23",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "cypress": "^15.3.0",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.5.0",
    "eslint-plugin-next": "^0.0.0",
    "eslint-plugin-storybook": "^9.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "playwright": "^1.54.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "storybook": "^9.1.1",
    "ts-jest": "^29.4.1",
    "uuid": "^11.1.0",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions"
  ]
}
