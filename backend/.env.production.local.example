# Backend Production Secrets
# Copy to .env.production.local and fill in actual values

# Database (REQUIRED)
DATABASE_URL="postgresql://prod_user:STRONG_PASSWORD@db-host:5432/loya_care_crm_prod"

# JWT Secret (REQUIRED - min 32 chars)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
JWT_SECRET="your-production-jwt-secret-min-32-characters"

# CORS (REQUIRED)
CORS_ORIGIN="https://your-production-domain.com"
