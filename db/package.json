{
  "name": "loya-crm-db",
  "version": "0.1.02",
  "private": true,
  "scripts": {
    "generate": "prisma generate && mkdir -p ../frontend/src/shared/generated/prisma/ && cp -R ./generated/prisma-client/* ../frontend/src/shared/generated/prisma/ && mkdir -p ../backend/generated/prisma/ && cp -R ./generated/prisma-client/* ../backend/generated/prisma/",
    "migrate": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --force",
    "migrate:status": "prisma migrate status",
    "studio": "prisma studio",
    "seed": "npx tsc && node temp/seed.js",
    "type-check": "tsc --noEmit",
    "lint": "echo 'No linting configured for db'",
    "lint:check": "echo 'No linting configured for db'"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^6.15.0",
    "@prisma/client": "^7.0.1",
    "@prisma/client-runtime-utils": "^7.0.1",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.4.5",
    "pg": "^8.13.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "latest",
    "@types/pg": "^8.15.6",
    "dotenv-cli": "^7.4.2",
    "prisma": "^7.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
